	private SqlSession session;
	
	public BoardDAO() {
		session = new MyConfig().getInstance();
	}

	public void insert() {
		int cnt = session.insert("board.dao.BoardDAO.insert");
		session.commit();
		System.out.println("ÃÑ " + cnt + "°³ Çà »ðÀÔ...");
	}
	
	public void work() {
		insert();
	}
}
